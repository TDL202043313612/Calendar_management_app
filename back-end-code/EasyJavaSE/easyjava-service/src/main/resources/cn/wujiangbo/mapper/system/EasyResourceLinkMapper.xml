<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.wujiangbo.mapper.system.EasyResourceLinkMapper">

    <select id="selectMySqlPage" resultType="cn.wujiangbo.domain.system.EasyResourceLink">
        select t1.*
        from easy_resource_link t1
        <where>
            <if test="query.keyword != null and query.keyword != '' ">
                 and t1.id like concat('%', #{query.keyword}, '%')
            </if>
        </where>
        order by t1.create_time desc
    </select>
</mapper>